{
  "name": "lubim-prod",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=6006",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "prettier": "npx prettier \"**/*.{ts,tsx}\" --write",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "reg-cli ./.loki/current ./.loki/reference ./.loki/difference -J ./.loki/report.json -R ./.loki/report.html",
    "storybook": "storybook dev -p 6009 -c ./config/storybook",
    "build:storybook": "storybook build -c ./config/storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.25.2",
